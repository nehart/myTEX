%
% The listings package is a source code printer for LATEX. You can typeset
% stand alone files as well as listings with an environment similar to verbatim
% as well as you can print code snippets using a command similar to \verb.
%
% https://wiki.math.ntnu.no/_media/drift/stud/listings.pdf
%

\RequirePackage{listings} % .. % load required package

%
% Defaul Listing configuration
% ----------------------------
% supported languages: Assembler; bash, C; C++; HTML; Mathematica; Octave; R; Ruby; Scilab; Swift; TeX; Ant; Awk; Basic; Fortran; Java; Perl; make; Pascal; PHP; Python; Scala; SQL, tcl; sh; XML; 
%

% ................................................................ %
\lstset{ % ....................................................... %
	language={}, % ............................................... % default programming language
	basicstyle=\tiny\ttfamily, % ........................... % the size of the fonts that are used for the code
	numberstyle=\tiny\color{lst_color_coding_numbers}, % ... % the size and color that are used for numbers
	keywordstyle=\color{lst_color_coding_keywords}, % ............ % the size and color that are used for keywords
	commentstyle=\color{lst_color_coding_comments}, % ............ % the size and color that are used for comments
	stringstyle=\color{lst_color_coding_strings}, % .............. % the size and color that are used for strings
	escapechar={\&}, % ........................................... % if you want to add LaTeX within your code (fe. ||\color{red})
	aboveskip=0mm, % ............................................. % vertical space added above code
	belowskip=0mm, % ............................................. % vertical space added below code
	columns=flexible, % .......................................... % all characters are typeset at their natural width
	numbers=none, % .............................................. % no linenumbers
	breaklines=true, % ........................................... % activates (true) automatic line breaking of long lines
	showstringspaces=false, % .................................... % don't show spaces in string as _
	tabsize=2, % ................................................. % sets default tabsize to 4 spaces
} % .............................................................. %
% ................................................................ %

%
% Number Listing configuration
%

% ................................. %
\lstdefinestyle{number} % ......... % style used if you want linnumbers (style=number); changes default style
{ % ............................... %
	numbers=left, % ............... % numbers left
	numbersep=8.5mm, % ............ % numbers 2mm from the left inside box
} % ............................... %
% ................................. %

% ................................. %
\lstdefinestyle{num} % ......... % style used if you want linnumbers (style=number); changes default style
{ % ............................... %
	numbers=left, % ............... % numbers left
	numbersep=8.5mm, % ............ % numbers 2mm from the left inside box
} % ............................... %
% ................................. %

